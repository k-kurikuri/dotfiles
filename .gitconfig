[user]
    name = {name}
    email = {name}@example.com
[core]
    editor = vim
[color]
    ui = auto
[alias]
    br = branch
    st = status
    di = diff
    co = checkout
    com = checkout master
    cl = clean
    plo = pull origin
    pso = push origin
    plu = pull upstream
    psu = push upstream
    plod = pull origin develop
    plud = pull upstream develop
    plom = pull origin master
    plum = pull upstream master
    cm = commit
    ad = add
    adu = add -u
    l = log
    lp = log -p
    lone = log --oneline
    lsta = log --stat
    dca = diff --cached
    mg = merge
    stas = stash -u
    stap = stash pop
    rb = rebase
    rbm = rebase master
    rbc = rebase --continue
    rbs = rebase --skip
